<script>



    var flag = true;
    $(document).ready(function () {
        $('#block').hide();
        $('#block1').hide();
        $('#block2').hide();
        var lastScrollTop = 0;
        $(window).scroll(function (event) {
            var st = $(this).scrollTop();
            if (st > lastScrollTop) {
                document.getElementById("main").innerHTML = st += 1;
                if (st >= 1100 && this.flag == true) {
                    this.flag = false
                    var _this = $('.default');
                    $('nav li a').removeClass('active');
                    $('.default').addClass('active');

                    $('#block').removeAttr('class');
                    $('#block').show();

                    $('#block').attr('class', _this.attr('href'));


                    //------------------
                    setTimeout(function () {
                        var _this1 = $('.default1');
                        $('nav li a').removeClass('active');
                        $('.default1').addClass('active');

                        $('#block1').removeAttr('class');
                        $('#block1').show();

                        $('#block1').attr('class', _this1.attr('href'));
                    }, 1000);

                    //------------------
                    setTimeout(function () {
                        var _this2 = $('.default2');
                        $('nav li a').removeClass('active');
                        $('.default2').addClass('active');

                        $('#block2').removeAttr('class');
                        $('#block2').show();

                        $('#block2').attr('class', _this2.attr('href'));
                    }, 2000);

                    //------------------
                    return;
                    //return false;
                }
            } else {
                if (st <= 378) {
                    this.flag = true;
                    $('#block').hide();
                    $('#block1').hide();
                    $('#block2').hide();
                }
                document.getElementById("main").innerHTML = st -= 1;
            }
            lastScrollTop = st;
        });
    });


</script>



